        "monitor": [
          {
            "x4_code": "(1-H-1(N,EL)1-H-1,,DA)",
            "free_txt": [
              " 180 degree n-p scattering (i.e. the H(n,p) reaction",
              " at zero degrees) was measured simultaneously for the",
              " 160 MeV data, using the magnetic spectrometer lisa."
            ]
          }
        ],
        "part-det": [
