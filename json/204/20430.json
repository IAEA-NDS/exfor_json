  "experimental_conditions": {
    "001": {
      "0": {
        "comment": [
          {
            "x4_code": null,
            "free_txt": [
              ".New method for determining p-wave assignment of U-238",
              " resonances presented consisting of measuring the",
              " fraction of capture gamma-rays above 4.3 MeV."
            ]
          }
        ],
        "detector": [
          {
            "x4_code": "(NAICR)",
            "free_txt": [
              " Two 7 x 6 inch NaI(Tl) detectors for measuring",
              " gammas above 4.3 and 1.5 MeV.",
              " Resolution 8.5% at 137-Cs line."
            ]
          },
          {
            "x4_code": "(SCIN)",
            "free_txt": [
              "    Liquid scintillator.",
              ".C(6)-F(6) capture gamma-ray detectors applying",
              " Maier-Leibnitz weighting method to obtain",
              " independence of the capture gamma-ray cascade."
            ]
          }
        ],
        "facility": [
          {
            "x4_code": "(LINAC,2ZZZGEL)",
            "free_txt": [
              "  Linear accelerator at Geel."
            ]
          }
        ],
        "method": [
          {
            "x4_code": "(TOF)",
            "free_txt": [
              "    50 m flight path."
            ]
          }
        ],
        "part-det": [
          {
            "x4_code": "(G)",
            "free_txt": [
              "    Gamma-rays."
            ]
          }
        ],
        "sample": [
          {
            "x4_code": null,
            "free_txt": [
              ".For measuring gammas above 4.3 and 1.5 MeV two runs",
              " made: one with 13.9 g/cm**2 U-238 plate (0.4 %",
              " U-235) of size 10x10 cm and inclined 45 degree to",
              " n-beam, the second with 4.64 g/cm**2 U-238 disc",
              " (0.2 % U-235) 12 cm diam. placed perpendicular to",
              " n-beam.",
              ".Capture cross-sections measured with 2.5 g/cm**2",
              " U-238(0.2 % U-235)"
            ]
          }
        ]
      }
    },
    "002": {
      "0": {
        "analysis": [
          {
            "x4_code": "(AREA)",
            "free_txt": [
              "   Area analysis. Program with Monte Carlo",
              " subroutine used for correcting multiple scattering",
              " contribution to area of resonance.",
              " Re-normalization factor was applied to the data such",
              " that the exp. areas for 14 strong s-wave resonances",
              " in the range 100-1700 eV are in agreement with the",
              " capture areas calculated with area analysis code from",
              " known resonance parameters of BNL-325,1973;",
              " the effective scattering radius 8.74 Fm was used.",
              " -  R-values were divided into two groups:",
              " 1) g*Gn/sqrt(En) > 0.15 meV",
              " 2) gGn/sqrt(En) < 0.15 meV.",
              " It was assumed that first group contains only s-wave",
              "resonances, while the 2nd group contains mostly p-wave",
              " but also a small number of s-wave resonances."
            ]
          }
        ],
        "assumed": [
          {
            "x4_code": "(ASSUM,92-U-238(N,EL)92-U-238,,RAD)",
            "free_txt": []
          }
        ],
