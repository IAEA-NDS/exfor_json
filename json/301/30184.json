  "experimental_conditions": {
    "001": {
      "0": {
        "analysis": [
          {
            "x4_code": null,
            "free_txt": [
              "The cross-sections are obtained by integration of the",
              "fully corrected gamma spectrum over the excitation",
              "energy interval up to the binding energy of the last",
              "neutron in the final nucleus."
            ]
          }
        ],
        "correction": [
          {
            "x4_code": null,
            "free_txt": [
              "Spectra are corrected",
              "  .For background",
              "  .For the spectrometer response function",
              "  .For the gammas created and absorbed in the sample",
              "  .For absorption of gammas in the paraffin-collimator",
              "  .For scattering of neutrons in the target"
            ]
          }
        ],
        "detector": [
          {
            "x4_code": "(BPAIR)",
            "free_txt": [
              "Scintillation pair spectrometer which resolution",
              "has been measured to be 12.5, 9.5 and 8.0 % for",
              "gammas of energies 12.1, 16.5 and 20.4MeV respectively.",
              "Efficiency, also measured for these gamma energies,",
              "has been found to be 0.45, 0.55 And 0.56 Per cent."
            ]
          }
        ],
        "exp-year": [
          {
            "x4_code": "(1968)",
            "free_txt": [
              " measurements from 1967 to 1970"
            ]
          }
        ],
        "err-analys": [
          {
            "x4_code": null,
            "free_txt": [
              "The overall error includes the uncertainty",
              "  .On the background correction= 15%",
              "  .On the energy scale= less than 200 keV at 14 MeV",
              "                        less than 400 keV at 22 MeV",
              "  .On the correction of absorption in sample=",
              "                         less than 15%",
              "  .On the correction for neutron scattering in the",
              "                            target= less than 10%",
              "  .Due to the spectrometer response function",
              "  .On the spectrometer efficiency= 12%",
              "  .On the neutron flux= 6%",
              "  .Due to the instability of electronics= 5%",
              "  .Due to statistical errors",
              "The resulting error for the cross-sections",
              "                              is about 15%",
              "The energy resolution is determined",
              "  .By the energy width of the spectrometer response",
              "   function (1.65+-0.05MeV)",
              "  .By the energy spread of the neutron beam (1.35 MeV)",
              "The resulting values are= 14% at 15 MeV",
              "                          10% at 21 MeV"
            ]
          }
        ],
        "facility": [
          {
            "x4_code": "(CCW)",
            "free_txt": [
              " Cockcroft-Walton accelerator",
              "energy of incident deuterons= 100 keV"
            ]
          }
        ],
        "inc-source": [
          {
            "x4_code": "(D-T)",
            "free_txt": [
              "  T(d,n)alpha reaction"
            ]
          }
        ],
        "inc-spect": [
          {
            "x4_code": null,
            "free_txt": [
              "The 'energy-spread' is 1.35 MeV, that is +-0.675 MeV"
            ]
          }
        ],
        "method": [
          {
            "x4_code": null,
            "free_txt": [
              "Gamma spectra are measured by the telescopic",
              "scintillation pair spectrometer.",
              "Recoil proton spectrometer has been used for the",
              "neutron monitoring."
            ]
          }
        ],
        "monitor": [
          {
            "x4_code": null,
            "free_txt": [
              "Absolute measurements"
            ]
          }
        ],
        "sample": [
          {
            "x4_code": null,
            "free_txt": [
              "Natural isotopic composition for all the targets",
              "targets are balls of 4-6 cm diameter placed around the",
              "tritium target therefore the neutron energy is spread",
              "over a range from 13.5 To 14.7 MeV.",
              "Due to the spherical geometry the spectra obtained are",
              "integrated over a solid angle of 4 pi."
            ]
          }
        ],
        "status": [
