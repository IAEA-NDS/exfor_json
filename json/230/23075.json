  "23075-001": {
    "extra_information": {
      "analysis": {
        "0": [
          {
            "x4_code": null,
            "free_txt": [
              "Spectrum averaged cross section was derived with an",
              " evaluated data library in the following procedure:",
              "1.C/E of reaction rate was obtained for the library.",
              "2.Representative neutron energy corresponding to 50%",
              "   of the reaction rate was calculated for the library.",
              "3.Cross section at the representative neutron energy",
              "   of the library was divided by the C/E to obtain",
              "   the experimental cross section."
            ]
          }
        ]
      },
      "correction": {
        "0": [
          {
            "x4_code": null,
            "free_txt": [
              "Corrected for",
              " - approximation of reactor power (<0.01%);",
              " - gamma-ray attenuation by quartz tube (1.7%-2.8%);",
              " - gamma-ray self-absorption in Au (2.0%);",
              " - self-shielding of neutrons by Au sample.",
              "Corrections were neglected for",
              " - sum coincidence;",
              " - neutron scattering in Au sample (<1%);",
              " - self-shielding of neutrons by Np sample."
            ]
          }
        ]
      },
