            ]
          }
        ],
        "method": [
          {
            "x4_code": "(TOF)",
            "free_txt": [
              "    TOF method for determination of the energy",
              " of incident neutrons and for discrimination of photons",
              " from neutron scattered by the sample."
            ]
          },
          {
            "x4_code": "(ASSOP)",
