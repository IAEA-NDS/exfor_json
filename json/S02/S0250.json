        "inc-source": {
          "0": [
            {
              "x4_code": null,
              "free_txt": [
                "The alpha beam intensity was about 100 - 200 enA for",
                "the on-line experiment. The alpha beam intensity for",
                "the on-line irradiation was about 1.0 euA, and it was",
                "monitored by a water-cooling Faraday cup."
              ]
            }
          ]
        },
