  "experimental_conditions": {
    "001": {
      "0": {
        "detector": [
          {
            "x4_code": "(TELES,SIBAR,SIBAR)",
            "free_txt": [
              "Reaction products were detected and",
              "identified by two surface-barrier (deltaE-E) detector",
              "telescopes mounted",
              "on two movable arms inside a one-meter diameter",
              "scattering chamber. The telescopes consisted of two",
              "deltaE Si-surface-barrier detectors (30 and 43.5",
              "micrometer thick) followed by two 300 micrometer thick",
              "E Si-surface-barrier detectors."
            ]
          },
          {
            "x4_code": "(SI)",
            "free_txt": [
              " The target was monitored by another Si-detector",
              " (300 micrometer thick) placed at 15 deg to the",
              "incident beam direction."
            ]
          }
        ],
        "facility": [
          {
            "x4_code": "(VDGT,3INDTAT)",
            "free_txt": [
              "14 UD Pelletron accelerator, TIFR,",
              "Bombay."
            ]
          }
        ],
        "inc-source": [
          {
            "x4_code": null,
            "free_txt": [
              "12-C.",
              "12C ions of 87.5 MeV incident energy were bombarded."
            ]
          }
        ],
        "method": [
          {
            "x4_code": "(EXTB,SITA,EDE)",
            "free_txt": [
              "A slow-fast coincidence set up was",
              "employed to achieve charge and mass separation of the",
              "light reaction products. The summed deltaE+E signals",
              "and the deltaE signals for each telescope were",
              "collected in 1024 x 512 matrices in a PC-based",
              "transputer system developed at the Bhabha Atomic",
              "Research Centre."
            ]
          }
        ],
        "monitor": [
          {
